<form class="form" action="/file/docx" method="post">
  <div class="form__input-container">
    {{#each tags}}
      <div class="form__input-wrapper">
        <label class="form__label" for="{{this}}">{{this}}</label>
        <input
          type="text"
          class="form__input tag-input"
          id="{{this}}"
          name="{{this}}"
        />
      </div>
    {{/each}}
  </div>

  <button
    class="form__btn form__btn--submit submit-btn"
    type="submit"
    disabled="true"
  >docx</button>
  <button
    class="form__btn form__btn--submit submit-btn"
    type="submit"
    formaction="/file/pdf"
    disabled="true"
  >pdf</button>
</form>